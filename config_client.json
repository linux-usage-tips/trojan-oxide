{
    "run_type": "client",
    "local_addr": "127.0.0.1",
    "local_port": 1080,
    "remote_addr": "yourdomain.com",
    "remote_port": 443,
    "password": [
        "your_secret_password"
    ],
    "websocket": {
        "enabled": true,
        "path": "/your-websocket-path",
        "hostname": "yourdomain.com"
    },
    "tls": {
        "enabled": true,
        "sni": "yourdomain.com",
        "verify": true,
        "alpn": ["h2", "http/1.1"]
    },
    "fast_open": true,
    "tcp_keepalive": true,
    "zero_copy": true,
    "log_level": "info"
}
